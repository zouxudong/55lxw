<script>
    var currentindex=1;
    var countFocus=3;
    var currentFocus=1;
    function changeflash(i) {
        currentFocus=i;
        currentindex=i;
        for (j=1;j<=countFocus;j++){
            if (j==i)
            {$("#flash"+j).fadeIn("normal");
                $("#flash"+j).css("display","block");
                $("#f"+j).removeClass();
                $("#f"+j).addClass("on");
            }
            else
            {$("#flash"+j).css("display","none");
                $("#f"+j).removeClass();
            }
        }}
    function startAm(){
        timerID = setInterval("timer_tick()",7000);
    }
    function stopAm(){
        clearInterval(timerID);
    }
    function timer_tick() {
        currentindex=currentindex>=countFocus?1:currentindex+1;
        changeflash(currentindex);}

    function timer_tickF() {
        currentFocus=currentFocus>=countFocus?1:currentFocus+1;
        changeflash(currentFocus);}

    $(document).ready(function(){
        $(".flashNum span").mouseover(function(){stopAm();}).mouseout(function(){startAm();});
        startAm();
    });

    function nextPage() {
        pageI = Number(currentFocus);
        if (pageI==countFocus)
        {i=1;}
        else
        {i=pageI+1;}
        changeflash(i)
    }
    function prePage() {
        pageI = Number(currentFocus);
        if (pageI==1)
        {i=countFocus;}
        else
        {i=pageI-1;}
        changeflash(i)
    }
</script>
